/* swiper处理 */
swiper {
    width: 100%;
    /* height: 100vh; */
    height: calc(100vh - 168.5rpx);
    /* height: calc(100vh - 168.5rpx); */
    padding: 0;
}
swiper-item {
    /* line-height: 350px; */
    /* overflow: scroll; */
    overflow: auto;
}
swiper-item::-webkit-scrollbar {
    display: none;
}

